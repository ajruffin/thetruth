<%= simple_form_for(@business) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :city %>
    <%= f.input :state %>
    <%= f.input :zip %>
    <%= f.input :image %>
    <%= f.association :user, label_method: :email, value_method: :id, include_blank: false%>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
